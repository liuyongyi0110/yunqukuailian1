<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yunqukuailian.app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".activity.SecuritycCenterActivity" />
        <activity android:name=".activity.LoginPasswordModificationActivity" />
        <activity android:name=".activity.TransactionDetailsActivity" />
        <activity android:name=".activity.IdentityAuthenticationActivity" />
        <activity android:name=".activity.PersionalRealNameActivity" />
        <activity android:name=".activity.RealNameAuthenticationActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.RegisteredActivity" />
        <activity android:name=".activity.ForgetPasswordActivity" />
        <activity android:name=".activity.AccountManagementActivity"></activity>
    </application>

</manifest>